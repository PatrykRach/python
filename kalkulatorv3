print("potęgowanie")
a=int(input("podaj pierwszą liczbe"))
b=int(input("podaj drugą liczbe"))
dzialanie= input("wybierz dzialanie")
if dzialanie == "1":
    print(a+b)
elif dzialanie == "2":
    print(a-b)
elif dzialanie == "3":
    print(a*b)
elif dzialanie == "4":
    if b!=0:
        print(a/b)
    else: 
        print("1")
elif dzialanie  == "5":
    c=int(input("podaj potęge"))
print(a*a)
